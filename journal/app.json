{
  "expo": {
    "name": "journal",
    "description": "Simple Journal App. Keep memories safe and secure without the paper.",
    "slug": "journal",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./Journal.png",
    "splash": {
      "image": "./Journal-Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tylersustare.journal"
    }
  }
}